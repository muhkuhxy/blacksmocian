---
import BaseLayout from "../layouts/BaseLayout.astro"
import Contact from "../sections/contact.astro"
import Masterminds from "../sections/masterminds.astro"

const gutter1 = '150px'
const gutter2 = '175px'
const endGutter = '175px'
const startGutter = parseInt(gutter1) + parseInt(gutter2) + 'px'
---

<BaseLayout>
  <div class="relative grid grid-cols-[var(--gutter1),var(--gutter2),auto,var(--endGutter)]">

    <section id="about" class="col-start-2 col-span-2">
      <Contact></Contact>
    </section>

  </div>
</BaseLayout>

<style define:vars={{ gutter1, gutter2, endGutter, startGutter }}>
</style>